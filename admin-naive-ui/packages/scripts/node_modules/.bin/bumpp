#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/bumpp@10.3.1_magicast@0.3.4/node_modules/bumpp/bin/node_modules:/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/bumpp@10.3.1_magicast@0.3.4/node_modules/bumpp/node_modules:/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/bumpp@10.3.1_magicast@0.3.4/node_modules:/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/bumpp@10.3.1_magicast@0.3.4/node_modules/bumpp/bin/node_modules:/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/bumpp@10.3.1_magicast@0.3.4/node_modules/bumpp/node_modules:/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/bumpp@10.3.1_magicast@0.3.4/node_modules:/mnt/d/panrenjie/Nest-Admin-main/admin-naive-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bumpp/bin/bumpp.mjs" "$@"
else
  exec node  "$basedir/../bumpp/bin/bumpp.mjs" "$@"
fi
